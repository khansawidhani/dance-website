doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        link(rel="stylesheet", href="/static/css/admin.css")
        title Admin Dashboard
    body 
        .container
            h1 Messages
            .contactInfo 
                table
                    tr 
                        th ID 
                        th Full Name 
                        th Email 
                        th Message 
                        th TimeStamp
                    each result in params
                        tr.data 
                            td= result._id
                            td= result.fullName 
                            td= result.email 
                            td= result.message
                            td=result.timeStamp
        